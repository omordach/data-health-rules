{
  "name": "unionimpact/data-health-poc",
  "description": "POC: Laravel data health checks (single-tenant)",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0"
  },
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.0",
    "pestphp/pest-plugin-laravel": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "UnionImpact\\DataHealthPoc\\": "src/"
    },
    "classmap": [
      "database/seeders"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "UnionImpact\\DataHealthPoc\\DataHealthPocServiceProvider"
      ]
    }
  },
  "scripts": {
    "test": "pest"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
